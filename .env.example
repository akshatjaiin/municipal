# Railway Environment Variables
# Copy this to your Railway project environment variables

# Django Settings
DEBUG=False
SECRET_KEY=your-super-secret-key-here-change-this-in-production
ALLOWED_HOSTS=*

# Database (Railway will provide these automatically)
# PGDATABASE=railway
# PGUSER=postgres
# PGPASSWORD=your-db-password
# PGHOST=containers-us-west-1.railway.app
# PGPORT=5432
# PGSSLMODE=require

# Optional: Railway provides these automatically
# DATABASE_URL=postgresql://...

# Admin Settings
DJANGO_SUPERUSER_USERNAME=admin
DJANGO_SUPERUSER_EMAIL=admin@municipal.com
DJANGO_SUPERUSER_PASSWORD=change-this-password